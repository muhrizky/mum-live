<odoo>
    <!-- Kode Contract -->
    <record id="contract_sequence_pkwt" model="ir.sequence">
        <field name="name">Kode Kontrak PKWT</field>
        <field name="code">kontrak_pkwt</field>
        <field eval="4" name="padding"/>
        <field name="company_id" eval="False"/>
        <field name="prefix">PKWT-</field>
        <field name="suffix">/MUM-*/%(month)s/%(year)s</field>
    </record>
    <record id="contract_sequence_phl" model="ir.sequence">
        <field name="name">Kode Kontrak PHL</field>
        <field name="code">kontrak_phl</field>
        <field eval="4" name="padding"/>
        <field name="company_id" eval="False"/>
        <field name="prefix">PHL-</field>
        <field name="suffix">/MUM-*/%(month)s/%(year)s</field>
    </record>
    <record id="contract_sequence_ppkwt" model="ir.sequence">
        <field name="name">Kode Kontrak PPKWT</field>
        <field name="code">kontrak_ppkwt</field>
        <field eval="4" name="padding"/>
        <field name="company_id" eval="False"/>
        <field name="prefix">PPKWT-</field>
        <field name="suffix">/MUM-*/%(month)s/%(year)s</field>
    </record>
    
    <record id="contract_sequence_pkwt_ext" model="ir.sequence">
        <field name="name">Kode Kontrak PKWT EXT</field>
        <field name="code">kontrak_pkwt_ext</field>
        <field eval="4" name="padding"/>
        <field name="company_id" eval="False"/>
        <field name="prefix">PKWT-</field>
        <field name="suffix">/MUM-EXT-*/%(month)s/%(year)s</field>
    </record>
    <record id="contract_sequence_phl_ext" model="ir.sequence">
        <field name="name">Kode Kontrak PHL EXT</field>
        <field name="code">kontrak_phl_ext</field>
        <field eval="4" name="padding"/>
        <field name="company_id" eval="False"/>
        <field name="prefix">PHL-</field>
        <field name="suffix">/MUM-EXT-*/%(month)s/%(year)s</field>
    </record>
    <record id="contract_sequence_ppkwt_ext" model="ir.sequence">
        <field name="name">Kode Kontrak PPKWT EXT</field>
        <field name="code">kontrak_ppkwt_ext</field>
        <field eval="4" name="padding"/>
        <field name="company_id" eval="False"/>
        <field name="prefix">PPKWT-</field>
        <field name="suffix">/MUM-EXT-*/%(month)s/%(year)s</field>
    </record>

    <!-- Kode NIP -->
    <record id="nip_sequence_int" model="ir.sequence">
        <field name="name">Kode NIP Internal</field>
        <field name="code">nip_internal</field>
        <field eval="4" name="padding"/>
        <field name="company_id" eval="False"/>
        <field name="suffix">.%(month)s.%(y)s.M</field>
    </record>
    <record id="nip_sequence_ext" model="ir.sequence">
        <field name="name">Kode NIP External</field>
        <field name="code">nip_external</field>
        <field eval="4" name="padding"/>
        <field name="company_id" eval="False"/>
        <field name="suffix">.%(month)s.%(y)s.MEXT</field>
    </record>
</odoo>